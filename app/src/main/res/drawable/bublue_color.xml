<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ceb58f"/>
            <stroke android:color="#ceb58f"
                android:width="2dp"
                />
            <corners android:radius="10dp"/>
            <padding android:bottom="8pt"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp"/>
        </shape>

    </item>

    <item  >
        <shape>
            <solid android:color="#f7e9c4"/>

            <stroke android:color="#f7e9c4"
                android:width="2dp"
                />
            <corners android:radius="10dp"/>
            <padding android:bottom="8pt"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp"/>
        </shape>

    </item>
</selector>